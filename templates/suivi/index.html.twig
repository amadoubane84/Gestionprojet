{% extends 'base.html.twig' %}

{% block title %}Suivi index{% endblock %}

{% block body %}
    <h1>Suivi index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Reunion_de_demarrage</th>
                <th>Contrat_de_travaux</th>
                <th>Ordre_de_service</th>
                <th>Implantation</th>
                <th>Dossiers_execution</th>
                <th>Pv_reception_provisoire</th>
                <th>Pv_levee_de_reserves</th>
                <th>Pv_reception_definitive</th>
                <th>Rapport_annuel</th>
                <th>Rapport_final</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for suivi in suivis %}
            <tr>
                <td>{{ suivi.id }}</td>
                <td>{{ suivi.reunionDeDemarrage }}</td>
                <td>{{ suivi.contratDeTravaux }}</td>
                <td>{{ suivi.ordreDeService }}</td>
                <td>{{ suivi.implantation }}</td>
                <td>{{ suivi.dossiersExecution }}</td>
                <td>{{ suivi.pvReceptionProvisoire }}</td>
                <td>{{ suivi.pvLeveeDeReserves }}</td>
                <td>{{ suivi.pvReceptionDefinitive }}</td>
                <td>{{ suivi.rapportAnnuel }}</td>
                <td>{{ suivi.rapportFinal }}</td>
                <td>
                    <a href="{{ path('suivi_show', {'id': suivi.id}) }}">show</a>
                    <a href="{{ path('suivi_edit', {'id': suivi.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="12">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('suivi_new') }}">Create new</a>
{% endblock %}
